<div class="hideonmobile">
  {{> ads/span/slim-dark}}
</div>

{{> header/header}}

<div class="body view-threecol">
  <div class="container">

    <section class="topspan">
      {{#each topspan}}
        {{{this}}}
      {{/each}}
    </section>

    <section class="main">

      <div class="col1">
        {{#each col1}}
          {{{this}}}
        {{/each}}
      </div>

      <div class="col2">
        {{#each col2}}
          {{#each this}}

            <div class="{{@key}}-{{this.length}}">
              {{#each this}}
                <div class="column">
                  {{{this}}}
                </div>
              {{/each}}
            </div>

          {{else}}
            {{{this}}}
          {{/each}}
        {{/each}}
      </div>

      <div class="col3">
        {{#each col3}}
          {{{this}}}
        {{/each}}
      </div>

    </section>

    <section class="bottomspan">
      {{#each bottomspan}}
        {{{this}}}
      {{/each}}
    </section>

    {{> ads/span/slim}}

  </div>
</div>

{{> footer/footer}}