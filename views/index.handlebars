{{> header/header}}

<div class="body view-homepage">
  <div class="container">

    <div class="row">
      <div class="column1 widget nostyle">
          {{{widget1}}}
      </div>
      <div class="column2 widget scrolloverflow">
        <div class="scrolloverflow-inside">
          {{{widget2}}}
        </div>
      </div>
    </div>

    <div class="row">
      <div class="column1">
        <div class="row">
          {{#each widget3}}
            <div class="column50 widget">
              {{{this}}}
            </div>
          {{/each}}
        </div>

        <div class="row">
          {{#each widget4}}
            <div class="column50 widget">
              {{{this}}}
            </div>
          {{/each}}
        </div>

        <div class="row">
          <div class="widget">
            {{{widget5}}}
          </div>
        </div>
      </div>

      <div class="column2 widget scrolloverflow">
        <div class="scrolloverflow-inside">
          row2:column2
        </div>
      </div>
    </div>

  </div>
</div>

{{> footer/footer}}