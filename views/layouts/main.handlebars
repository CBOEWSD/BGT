<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <title>{{title}} - CBOE</title>
  {{#meta}}
    {{#keywords}}<meta name="keywords" content="{{this}}" >{{/keywords}}
    {{#description}}<meta name="description" content="{{this}}" >{{/description}}
  {{/meta}}

  <!-- Responsive -->
  <!--<meta name="viewport" content="initial-scale=1.0, user-scalable=no">-->
  <meta name="viewport" content="width=100%; initial-scale=1; maximum-scale=1; minimum-scale=1; user-scalable=no;" />

  {{{renderStyles 'css'}}}

  <!--
    Switch debug mode for additional logging -->
  <script>
    window.debug = {{isProd}};
  </script>

  <!--
    SHIMS
    These fix issues in legacy browsers where certain
    features are not supported.
  -->
  <!--[if lt IE 10]>
  <script src="/ui/classes/shims/ie-lt10.js"></script>
  <![endif]-->
  <!--[if lt IE 9]>
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <script src="/ui/libs/respond/dest/respond.src.js"></script>
  <![endif]-->

</head>
<body>

  {{{body}}}

  <!-- START - Load Qunit tests when required -->
  <script>
  (function () {
    var url = window.location.href,
        test = (url.indexOf('#test=on') !== -1) ? true : false;

    if (test) {
      document.write('<script src="http://code.jquery.com/qunit/qunit-1.12.0.js"\></script\>');
      document.write('<link href="http://code.jquery.com/qunit/qunit-1.12.0.css" media="all" rel="stylesheet" type="text/css"\>');
    }
  }).call(this);
  </script>
  <!-- END - Qunit -->

  {{{renderScripts 'libs'}}}
  {{{renderScripts 'initCoffee'}}}

</body>
</html>
