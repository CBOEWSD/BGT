<div class="wrap-columns-outer">
  {{#if meta}}
  <div class="wrap-meta">
    {{#each meta.items}}
      <div class="wrap-meta-item{{#this.class}} {{this}}{{/this.class}}">
        {{#each this.content}}
        <span class="item">
        {{#if this.label}}<strong>{{this.label}}</strong>{{/if}}
        {{this.text}}
        </span>

        {{else}}
        {{this.content}}
        {{/each}}
      </div>
    {{/each}}
  </div>
  {{/if}}
  <div class="wrap-columns-inner two {{#each classes}} {{this}} {{/each}}">
    <div class="column {{#if widgetWrapper}} widget {{/if}} {{#each column1.classes}} {{this}} {{/each}}">
      {{#each column1.modules}}
        <div class="component">
          {{{this}}}
        </div>
      {{/each}}
    </div>
    <div class="column {{#if widgetWrapper}} widget {{/if}} {{#each column2.classes}} {{this}} {{/each}}">
      {{#each column2.modules}}
        <div class="component">
          {{{this}}}
        </div>
      {{/each}}
    </div>
  </div>
</div>